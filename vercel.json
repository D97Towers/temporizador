{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/admin/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/children(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/games(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/sessions(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/test",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
